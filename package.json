{
  "name": "cfd-csharp",
  "version": "0.2.0",
  "description": "cfd-csharp",
  "scripts": {
    "code_format": "clang-format -i --style=file src/*.cpp src/*.h include/cfd/*.h src/capi/*.cpp src/capi/*.h include/cfdc/*.h",
    "cmake_make_install": "run-s cmake_clean cmake_release_make cmake_make_install_pkg",
    "cmake_install": "run-s cmake_clean cmake_release cmake_install_pkg",
    "cmake_install_pkg": "run-script-os",
    "cmake_install_pkg:win32": "cmake --install build",
    "cmake_install_pkg:linux": "sudo cmake --install build",
    "cmake_install_pkg:darwin": "sudo cmake --install build",
    "cmake_make_install_pkg": "run-script-os",
    "cmake_make_install_pkg:win32": "cmake --install build",
    "cmake_make_install_pkg:linux": "cd build && sudo make install",
    "cmake_make_install_pkg:darwin": "cd build && sudo make install",
    "cmake_release": "run-s cmake_release_configure cmake_build_parallel",
    "cmake_release_configure": "run-script-os",
    "cmake_release_configure:win32": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_TESTS=off --CDENABLE_JS_WRAPPER=off",
    "cmake_release_configure:linux": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_TESTS=off --CDENABLE_JS_WRAPPER=off --CDTARGET_RPATH=./build/Release -g",
    "cmake_release_configure:darwin": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_TESTS=off --CDENABLE_JS_WRAPPER=off --CDTARGET_RPATH=\"@executable_path;./build/Release\" -C",
    "cmake_release_make": "run-s cmake_release_make_configure cmake_build_parallel",
    "cmake_release_make_configure": "run-script-os",
    "cmake_release_make_configure:win32": "cmake -S . -B build -G \"Visual Studio 16 2019\" -A x64 -DENABLE_SHARED=on -DENABLE_TESTS=on -DENABLE_JS_WRAPPER=off -DENABLE_CAPI=on -DCMAKE_BUILD_TYPE=Release -DCMAKE_GENERATOR_PLATFORM=x64",
    "cmake_release_make_configure:linux": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_TESTS=off --CDENABLE_JS_WRAPPER=off --CDTARGET_RPATH=./build/Release -g -m",
    "cmake_release_make_configure:darwin": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_TESTS=off --CDENABLE_JS_WRAPPER=off --CDTARGET_RPATH=\"@executable_path;./build/Release\" -C -m",
    "cmake_all": "run-s cmake_configure cmake_build_parallel",
    "cmake_configure": "run-script-os",
    "cmake_configure:win32": "cmake -S . -B build -G \"Visual Studio 16 2019\" -A x64 -DENABLE_SHARED=on -DENABLE_JS_WRAPPER=off -DENABLE_CAPI=on -DCMAKE_BUILD_TYPE=Release -DCMAKE_GENERATOR_PLATFORM=x64",
    "cmake_configure:linux": "cmake -S . -B build -G \"Unix Makefiles\" -DENABLE_SHARED=on -DENABLE_JS_WRAPPER=off -DENABLE_CAPI=on -DCMAKE_BUILD_TYPE=Release -DTARGET_RPATH=./build/Release",
    "cmake_configure:darwin": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_JS_WRAPPER=off -DTARGET_RPATH=\"@executable_path;./build/Release\" -C",
    "cmake_quiet": "run-s cmake_quiet_configure cmake_quiet_build",
    "cmake_quiet_configure": "run-script-os",
    "cmake_quiet_configure:win32": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_JS_WRAPPER=off -i",
    "cmake_quiet_configure:linux": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_JS_WRAPPER=off --CDTARGET_RPATH=./build/Release -g -i",
    "cmake_quiet_configure:darwin": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_JS_WRAPPER=off --CDTARGET_RPATH=\"@executable_path;./build/Release\" -C -i",
    "cmake_elem_off": "run-s cmake_elem_off_configure cmake_build",
    "cmake_elem_off_configure": "run-script-os",
    "cmake_elem_off_configure:win32": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_ELEMENTS=off --CDENABLE_JS_WRAPPER=off",
    "cmake_elem_off_configure:linux": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_ELEMENTS=off --CDENABLE_JS_WRAPPER=off --CDTARGET_RPATH=./build/Release -g",
    "cmake_elem_off_configure:darwin": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_ELEMENTS=off --CDENABLE_JS_WRAPPER=off --CDTARGET_RPATH=\"@executable_path;./build/Release\" -C",
    "cmake_log_all": "run-s cmake_log_configure cmake_build",
    "cmake_log_configure": "run-script-os",
    "cmake_log_configure:win32": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_JS_WRAPPER=off --CDCFDCORE_DEBUG=on",
    "cmake_log_configure:linux": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_JS_WRAPPER=off --CDTARGET_RPATH=./build/Release --CDCFDCORE_DEBUG=on -g",
    "cmake_log_configure:darwin": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_JS_WRAPPER=off --CDTARGET_RPATH=\"@executable_path;./build/Release\" --CDCFDCORE_DEBUG=on -C",
    "cmake_con_all": "run-s cmake_con_configure cmake_build",
    "cmake_con_configure": "run-script-os",
    "cmake_con_configure:win32": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_JS_WRAPPER=off --CDCFDCORE_DEBUG=on --CDCFDCORE_LOG_CONSOLE=on",
    "cmake_con_configure:linux": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_JS_WRAPPER=off --CDTARGET_RPATH=./build/Release --CDCFDCORE_DEBUG=on --CDCFDCORE_LOG_CONSOLE=on -g",
    "cmake_con_configure:darwin": "cmake-js configure -O build --CDENABLE_SHARED=on --CDENABLE_JS_WRAPPER=off --CDTARGET_RPATH=\"@executable_path;./build/Release\" --CDCFDCORE_DEBUG=on --CDCFDCORE_LOG_CONSOLE=on -C",
    "cmake_debug_all": "run-s cmake_debug_configure cmake_debug_build",
    "cmake_debug_configure": "run-script-os",
    "cmake_debug_configure:win32": "cmake-js configure -O build --CDCFDCORE_DEBUG=on --CDCFDCORE_DEBUG=on --CDCFDCORE_LOG_CONSOLE=on -D",
    "cmake_debug_configure:linux": "cmake-js configure -O build --CDCFDCORE_DEBUG=on --CDTARGET_RPATH=./build/Debug -g -D",
    "cmake_debug_configure:darwin": "cmake-js configure -O build --CDCFDCORE_DEBUG=on --CDTARGET_RPATH=\"@executable_path;./build/Debug\" -C -D",
    "cmake_build_direct": "cmake --build build --parallel 4",
    "cmake_build": "cmake-js build -O build",
    "cmake_build_parallel": "cmake --build build --parallel --config Release",
    "cmake_debug_build_parallel": "cmake --build build --parallel --config Debug",
    "cmake_debug_build": "cmake-js build -O build -D",
    "cmake_quiet_build": "cmake-js build -O build -i",
    "cmake_clean": "cmake-js clean -O build",
    "test_all": "run-s ctest",
    "ctest": "cd build && ctest -C Release",
    "ctest_debug": "cd build && ctest -C Debug",
    "ctest_quiet": "cd build && ctest --quiet -C Release",
    "test_cfd": "run-script-os",
    "test_cfd:win32": "cd build/Release && cfd_test.exe",
    "test_cfdc:linux": "cd build/Release && ./cfd_test",
    "test_cfd:darwin": "cd build/Release && ./cfd_test",
    "doxygen_check": "run-script-os",
    "doxygen_check:win32": "cd doc && doxygen Doxyfile_quiet_all && rmdir /S /Q html_all_quiet",
    "doxygen_check:linux": "cd doc && doxygen Doxyfile_quiet_all && rm -rf html_all_quiet",
    "doxygen_check:darwin": "cd doc && doxygen Doxyfile_quiet_all && rm -rf html_all_quiet",
    "check": "run-s doxygen_check ctest_quiet lint_check",
    "check_all": "run-s cmake_clean git_subm_update cmake_quiet doxygen_check ctest_quiet lint_check"
  },
  "author": "",
  "license": "MIT",
  "gypfile": false,
  "dependencies": {
    "cmake-js": "^5.2.0",
    "big-integer": "^1.6.26",
    "bindings": "*",
    "nan": "^2.11.0",
    "node-gyp": "^3.8.0",
    "tape": "^4.8.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "run-script-os": "^1.0.5"
  }
}
